{
  "compilerOptions": {
    "rootDirs": ["src"],
    "outDir": "dist",
    "lib": ["es2020"],
    "target": "es2020",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "types": ["node"],
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noEmit": true,
    "noEmitHelpers": false
  }
}
